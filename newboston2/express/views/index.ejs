<!DOCTYPE html>
<html>
  <head>
    <title>The Dullahan</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>

    <% include templates/header.ejs %>
    
    <p><%= name %></p>

    <p><%= points %></p>

    <ul>
      <% for(let i = 0; i < 5; i++){ %>
        <li>Trigun</li>
      <% } %>
    </ul>

    <h3><%= videodata.categoryName %></h3>

    <ul>
      <% videodata.categories.forEach(function(item){ %>
        <li><a href="https://www.thenewboston.com/videos_food.php?cat=<%= item.categoryID %>"><%= item.categoryName %></a></li>
      <% }) %>
    </ul>

  </body>
</html>
